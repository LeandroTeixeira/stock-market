{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run restore-test && jest --watchAll",
    "test-coverage": "npm run restore-test && jest --watchAll --coverage",
    "restore": "npx sequelize db:migrate:undo:all --env development && npx sequelize db:migrate --env development && npx sequelize db:seed:all --env development",
    "restore-test": "npx sequelize db:migrate:undo:all && npx sequelize db:migrate && npx sequelize db:seed:all ",
    "dev": "npm run restore && nodemon src/index.js",
    "start": "npm run restore && node src/index.js",
    "generator": "node ./src/data/converter.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html",
      "text"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/data/*.js"
    ]
  },
  "devDependencies": {
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.3",
    "sinon": "^14.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.4.4",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.19",
    "random": "^3.0.6",
    "sequelize": "^6.21.3",
    "sequelize-cli": "^6.4.1",
    "supertest": "^6.2.4"
  }
}
